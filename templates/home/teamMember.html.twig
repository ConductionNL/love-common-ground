{# templates/home/modal.html.twig #}
	
                  <!-- Team member -->
                  <div class="col-md-4 col-sm-6 team panel">
                    <div class="panel-body">
                      <div>
                        <div class="team-image">
                          <img src="{{ member.image }}" alt="">
                        </div>
                        <div class="team-title">
                          <h4 class="color-2">{{ member.name }}</h4>
                          <div class="sub-title">{{ member.function }}</div>
                        </div>
                      </div>
                      <div>
                        <hr />
                        <p>{{ member.description }}</p>                        
                      </div>
                    </div>
                  </div>